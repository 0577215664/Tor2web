#! /usr/bin/python
# -*- encoding: utf-8 -*-

import tor2web

import os
import sys
from subprocess import call

t2w_worker = os.path.abspath(os.path.join(os.path.dirname(tor2web.__file__), 't2w-worker.py'))

args = ["python", t2w_worker]
args += sys.argv[1:]

#print len(args)

try:
    exit(call(args))
except:
    pass

exit(0)
